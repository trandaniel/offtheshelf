<html>
<head>
  <meta charset="utf-8">
  <title>OffTheShelf!</title>
  <link href="https://fonts.googleapis.com/css?family=Montserrat" rel="stylesheet">
  <link rel="stylesheet" href="styles.css" charset="utf-8">
</head>
<body>
  <% include navbar/nav %>
  <div class="container">
    <img src="img/blackfont.png" alt="logo"> <br>
    <div>
      Find what you're looking for!
      <form action="search" method="POST">
        <input type="text" class="form-control" name="searchTerm" placeholder="Search" required>
        <% if(hello) { %>
          <% include error/hello %>
        <% } %>
        <button type="submit" class="btn btn-default searchbutton" name="button">Search!</button>
      </form>
    </div>
  </div>
<% include maps/indexMap.ejs %>
</body>
</html>
